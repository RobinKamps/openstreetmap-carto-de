  - id: landuse-overlay
    geometry: polygon
    <<: *extents
    Datasource:
      <<: *osm2pgsql
      table: |-
        (SELECT
            way,
            landuse,
            military,
            way_area/NULLIF(!pixel_width!::real*!pixel_height!::real,0) AS way_pixels
          FROM planet_osm_polygon
          WHERE landuse = 'military'
            OR military = 'danger_area'
            AND building IS NULL
        ) AS landuse_overlay
    properties:
      minzoom: 8
